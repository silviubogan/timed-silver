<Control x:Class="cs_timed_silver.ImageFileSelector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:cs_timed_silver"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="400"
             xmlns:xceed="http://schemas.xceed.com/wpf/xaml/toolkit"
             Loaded="UserControl_Loaded"
             
             AllowDrop="True"
             PreviewDragEnter="UserControl_PreviewDragEnter"
             PreviewDragLeave="UserControl_PreviewDragLeave"
             PreviewDrop="UserControl_PreviewDrop"
             
             Template="{DynamicResource ImageFileSelectorTemplate}">
</Control>
